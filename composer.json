{
    "name": "rszrama/negotiation-middleware",
    "type": "library",
    "description": "Content negotiation middleware for PHP applications using a request, response, and next callable signature.",
    "keywords": ["negotiation", "middleware", "slim"],
    "homepage": "https://github.com/rszrama/negotiation-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Szrama",
            "email": "ryan.szrama@gmail.com",
            "homepage": "http://ryanszrama.com/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "psr/http-message": "1.0",
        "slim/slim": "3.0.0-RC1",
        "willdurand/negotiation": "2.0.0-alpha1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Gofabian\\Negotiation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gofabian\\Negotiation\\": "tests/"
        }
    }
}
